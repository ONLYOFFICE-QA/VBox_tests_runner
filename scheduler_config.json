{
    "start_hour": 2,
    "end_hour": 12,
    "interval_minutes": 30,
    "builder_run_cmd": "uv run inv builder-test -h -p 3 --version {version} --connect-portal --telegram",
    "desktop_run_cmd": "uv run inv desktop-test -d -h -p 5 --version {version} --telegram",
    "base_version": "9.0.4",
    "max_builds": 50,
    "recheck_count": 2,
    "recheck_all": false,
    "tested_versions_file": "./reports/report_checker/tested_versions.json",
    "cache_max_versions": 10,
    "any_package_exists": false,
    "test_execution_order": [
        "builder",
        "desktop"
    ]
}
